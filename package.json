{
  "name": "@kittycad/lib",
  "version": "3.0.4",
  "description": "Javascript library for KittyCAD API",
  "type": "module",
  "keywords": [
    "hardware",
    "library",
    "CAD",
    "Computer aided design",
    "mechanical engineering",
    "typescript"
  ],
  "homepage": "https://github.com/KittyCAD/kittycad.ts#readme",
  "bugs": {
    "url": "https://github.com/KittyCAD/kittycad.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KittyCAD/kittycad.ts.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "browser": "./dist/mjs/index.js",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/mjs/index.js",
  "browser": "./dist/mjs/index.js",
  "types": "./dist/types/src",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 16.13"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@biomejs/biome": "^1.9.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "~20",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "~8.29",
    "@typescript-eslint/parser": "~8.29",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "^28.8.3",
    "fast-json-patch": "^3.1.1",
    "handlebars": "^4.7.8",
    "prettier": "^2.8.1",
    "rimraf": "~3.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsutils": "~3.21",
    "typescript": "~4.7",
    "vitest": "^0.33.0"
  },
  "scripts": {
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "rimraf dist && npm run build:types && npm run build:js",
    "gen": "node --loader ts-node/esm ./gen/modelsGen.ts && yarn fmt",
    "test": "vitest",
    "tsc": "tsc",
    "fmt": "biome format --write ."
  },
  "author": "Kurt Hutten <kurt@zoo.dev>",
  "license": "MIT",
  "dependencies": {
    "bson": "^6.8.0",
    "cross-fetch": "^4.0.0",
    "openapi-types": "^12.0.0",
    "ts-node": "^10.9.1",
    "tslib": "~2.8",
    "ws": "^8.17.0"
  },
  "optionalDependencies": {
    "win-ca": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
